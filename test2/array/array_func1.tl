
#OUTPUT
#242
#Size: 10
#END

#RET 0

func printArray(numbers:i32[], size:i32) is
    var x : i32 := numbers[2];
    
    println("%d", x);
    println("Size: %d", size);
end

func main -> i32 is
    var numbers : i32[10];
    numbers[2] := 242;
    
    printArray(numbers, 10);
    
    return 0;
end

